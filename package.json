  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/data": "next",
    "@grafana/toolkit": "next",
    "@grafana/runtime": "next",
    "@grafana/ui": "next",
    "@types/lodash": "latest"
  },
  "engines": {
    "node": ">=12 <13"
  }
}
